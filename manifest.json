{
    "manifest_version": 2,
    "version":"1.0",
    "name": "2QR-Code",
    "browser_action": {
        "default_icon": { // optional
            "16": "images/icon16.png", // optional
            "24": "images/icon24.png", // optional
            "32": "images/icon32.png", // optional
            "64": "images/icon64.png" // optional
        },
        "default_title": "2QR-Code", // optional; shown in tooltip
        "default_popup": "index.html" // optional
    },
    "permissions": [
        "tabs"
    ]
}